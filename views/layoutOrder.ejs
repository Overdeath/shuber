<script>
  $(function() {

    $( "#pickup-date-start" ).datetimepicker({
      defaultDate: "+1w",
      changeMonth: true,
      numberOfMonths: 1,
      onClose: function( selectedDate ) {
        $( "#pickup-date-end" ).datetimepicker( "option", "minDate", selectedDate );
      }
    });
    $( "#pickup-date-end" ).datetimepicker({
      defaultDate: "+1w",
      changeMonth: true,
      numberOfMonths: 1,
      onClose: function( selectedDate ) {
        $( "#pickup-date-start" ).datetimepicker( "option", "maxDate", selectedDate );
      }
    });
    $( "#delivery-date-start" ).datetimepicker({
      defaultDate: "+1w",
      changeMonth: true,
      numberOfMonths: 1,
      onClose: function( selectedDate ) {
        $( "#delivery-date-end" ).datetimepicker( "option", "minDate", selectedDate );
      }
    });
    $( "#delivery-date-end" ).datetimepicker({
      defaultDate: "+1w",
      changeMonth: true,
      numberOfMonths: 1,
      onClose: function( selectedDate ) {
        $( "#delivery-date-start" ).datetimepicker( "option", "maxDate", selectedDate );
      }
    });

    // <img src="../assets/bike.png" class="img-package-size" id="package-small" data-id="small" />

    $( ".img-package-size").on("click", function () {
      $( ".img-package-size").removeClass('img-package-size-selected');
      $(this).addClass('img-package-size-selected');
    });

    $('#pickup-map').locationpicker({
        location: {latitude: 44.413167, longitude: 26.137822},
        radius: 0,
        inputBinding: {
            latitudeInput: $('#pickup-lat'),
            longitudeInput: $('#pickup-lon'),
            radiusInput: $('#pickup-radius'),
            locationNameInput: $('#pickup-address')
        },
        enableAutocomplete: true
    });


    $('#delivery-map').locationpicker({
        location: {latitude: 44.424334, longitude: 26.060985},
        radius: 0,
        inputBinding: {
            latitudeInput: $('#delivery-lat'),
            longitudeInput: $('#delivery-lon'),
            radiusInput: $('#delivery-radius'),
            locationNameInput: $('#delivery-address')
        },
        enableAutocomplete: true
    });
  });
</script>


